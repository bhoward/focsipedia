{
  "name": "focsipedia",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider docusaurus start",
    "build": "node --openssl-legacy-provider --max_old_space_size=16000 node_modules/@docusaurus/core/bin/docusaurus.mjs build",
    "swizzle": "NODE_OPTIONS=--openssl-legacy-provider docusaurus swizzle",
    "deploy": "node --openssl-legacy-provider --max_old_space_size=16000 node_modules/@docusaurus/core/bin/docusaurus.mjs deploy"
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.3",
    "@docusaurus/preset-classic": "^2.4.3",
    "classnames": "^2.5.1",
    "clipboard": "^2.0.11",
    "codemirror": "^5.58.2",
    "create-react-context": "0.2.2",
    "prism-react-renderer": "^1.1.1",
    "prismjs": "^1.27.0",
    "react": "^16.8.4",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^16.8.4",
    "react-simple-code-editor": "^0.10.0",
    "rehype-katex": "^2.0.0",
    "remark-custom-blocks": "^2.4.5",
    "remark-grid-tables": "^2.0.2",
    "remark-math": "^2.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
